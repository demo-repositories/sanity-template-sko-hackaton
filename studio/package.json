    "@sanity/vision": "^5.1.0",
    "date-fns": "^4.1.0",
    "lodash.get": "^4.4.2",
    "pluralize-esm": "^9.0.5",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "rxjs": "^7.8.2",
    "sanity": "^5.1.0",
    "sanity-plugin-asset-source-unsplash": "^7.0.0",
